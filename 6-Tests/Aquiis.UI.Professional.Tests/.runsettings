<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <Playwright>
    <BrowserName>chromium</BrowserName>
    <LaunchOptions>
      <Headless>false</Headless>
      <SlowMo>10000</SlowMo>
    </LaunchOptions>
    <ContextOptions>
      <IgnoreHTTPSErrors>true</IgnoreHTTPSErrors>
      <BaseURL>http://localhost:5197</BaseURL>
    </ContextOptions>
  </Playwright>
  <RunConfiguration>
    <EnvironmentVariables>
      <ASPNETCORE_ENVIRONMENT>Development</ASPNETCORE_ENVIRONMENT>
      <PWDEBUG>1</PWDEBUG>
    </EnvironmentVariables>
  </RunConfiguration>
</RunSettings>
